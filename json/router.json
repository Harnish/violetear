{
    "versions": [
        "v0",
        "v1",
        "v2"
    ],
    "hosts": [{
        "host": "*",
        "vroot": "default"
    }, {
        "host": "*.zunzun.io",
        "vroot": "default"
    }, {
        "host": "ejemplo.org",
        "vroot": "ejemplo"
    }, {
        "host": "api.ejemplo.org",
        "vroot": "ejemplo"
    }],
    "handlers": {
        "default": [{
            "url": "/test/.*",
            "handler": "my_handler",
            "methods": [
                "GET",
                "POST",
                "PUT"
            ]
        }, {
            "url": "/(md5|sha1|sha256|sha512)(/.*)?",
            "handler": "hash_handler",
            "methods": [
                "GET"
            ]
        }, {
            "url": "/.*",
            "handler": "default"
        }],
        "ejemplo": [{
            "url": "/.*",
            "handler": "other_handler",
            "methods": [
                "ALL"
            ]
        }]
    }
}
