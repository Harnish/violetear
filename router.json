{
  "vroot_1": {
    "/(md5|sha1|sha256|sha512)(/.*)?": [
      "handler",
      [
        "GET",
        "POST"
      ]
    ],
    "/upload/?.*": [
      "upload",
      [
        "PUT",
        "POST"
      ]
    ]
  },
  "vroot_2": {
    "/.*": [
      "default_handler"
    ],
    "/test": [
      "test_handler",
      "POST, GET"
    ]
  }
}
